<h1><%%= title %></h1>

<%% if collection.any? %>
  <div class="<%= HanamiId.pluralize model %>">
    <%% collection.each do |resource| %>
      <div class="<%= model %>">
        <h2><%%= resource.email %></h2>
        <p><%%= resource.created_at %></p>
      </div>
    <%% end %>
  </div>
<%% else %>
  <p class="placeholder"><%%= t("hanami_id.collections.nothing_yet", models: "<%= HanamiId.pluralize model %>") %></p>
<%% end %>
